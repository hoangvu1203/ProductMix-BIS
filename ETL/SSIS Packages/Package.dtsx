                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH RFM_Calculated AS (
    SELECT 
        CustomerID,
        DATEDIFF(DAY, MAX(OrderDate), GETDATE()) AS Recency, -- Days since the last order
        COUNT(SalesOrderID) AS Frequency,                    -- Total number of orders
        SUM(TotalDue) AS Monetary,                           -- Total amount spent
        MAX(ModifiedDate) AS ModifiedDate                -- Latest ModifiedDate
    FROM Sales.SalesOrderHeader
    GROUP BY CustomerID
),
RFM_Normalized AS (
    SELECT 
        CustomerID,
        NTILE(5) OVER (ORDER BY Recency ASC) AS RecencyScore,    -- Rank Recency into 5 groups (1=most recent)
        NTILE(5) OVER (ORDER BY Frequency DESC) AS FrequencyScore, -- Rank Frequency into 5 groups (5=highest frequency)
        NTILE(5) OVER (ORDER BY Monetary DESC) AS MonetaryScore,  -- Rank Monetary into 5 groups (5=highest spend)
        ModifiedDate
    FROM RFM_Calculated
)
SELECT 
    CustomerID,
    0.15 * RecencyScore + 0.28 * FrequencyScore + 0.57 * MonetaryScore AS RFMScore,
    RecencyScore,
    FrequencyScore,
    MonetaryScore,
    ModifiedDate
FROM RFM_Normalized
ORDER BY RFMScore DESC;</property>
