                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH PeriodSales AS (
    SELECT
        sod.SalesOrderDetailID,
        sod.ProductID,
        soh.CustomerID,
        soh.OrderDate,
        YEAR(soh.OrderDate) AS SalesYear,
        MONTH(soh.OrderDate) AS SalesMonth,
        SUM(sod.OrderQty) AS TotalQtySold,
        SUM(sod.OrderQty * sod.UnitPrice) AS SalesRevenue, -- Units Sold × Price Per Unit
        SUM(sod.OrderQty * p.StandardCost) AS COGS,         -- Units Sold × Standard Cost
		MAX(COALESCE(soh.ModifiedDate, sod.ModifiedDate, '1900-01-01')) AS ModifiedDate
    FROM Sales.SalesOrderDetail sod
    INNER JOIN Sales.SalesOrderHeader soh
        ON sod.SalesOrderID = soh.SalesOrderID
    INNER JOIN Production.Product p
        ON sod.ProductID = p.ProductID
    GROUP BY sod.SalesOrderDetailID, sod.ProductID, soh.CustomerID, soh.OrderDate, YEAR(soh.OrderDate), MONTH(soh.OrderDate)
),
GrowthAndASP AS (
    SELECT 
        ps.*,
        -- Calculate Gross Profit
        (ps.SalesRevenue - ps.COGS) AS GrossProfit,
        -- Calculate Sales Volume Growth Rate (SVGR)
        LAG(ps.TotalQtySold) OVER (PARTITION BY ps.ProductID ORDER BY ps.SalesYear, ps.SalesMonth) AS PriorPeriodSales,
        CASE 
            WHEN LAG(ps.TotalQtySold) OVER (PARTITION BY ps.ProductID ORDER BY ps.SalesYear, ps.SalesMonth) &gt; 0 THEN
                ((ps.TotalQtySold - LAG(ps.TotalQtySold) OVER (PARTITION BY ps.ProductID ORDER BY ps.SalesYear, ps.SalesMonth)) 
                * 100.0 / LAG(ps.TotalQtySold) OVER (PARTITION BY ps.ProductID ORDER BY ps.SalesYear, ps.SalesMonth))
            ELSE 0
        END AS SVGR,
        -- Calculate Average Selling Price (ASP)
        CASE 
            WHEN ps.TotalQtySold &gt; 0 THEN ps.SalesRevenue * 1.0 / ps.TotalQtySold
            ELSE 0
        END AS ASP
    FROM PeriodSales ps
)
SELECT 
    SalesOrderDetailID,
    ProductID,
    CustomerID,
    OrderDate,
    TotalQtySold,
    SalesRevenue,
    GrossProfit,
    SVGR,
    ASP,
    MAX(ModifiedDate) AS ModifiedDate
FROM GrowthAndASP
GROUP BY SalesOrderDetailID, ProductID, CustomerID, OrderDate, TotalQtySold, SalesRevenue, GrossProfit, SVGR, ASP;</property>
